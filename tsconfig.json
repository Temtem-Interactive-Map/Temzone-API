{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": ["esnext"],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "types": ["@cloudflare/workers-types"],
    "baseUrl": "src",
    "paths": {
      "database/*": ["../database/*"],
      "test/*": ["../test/*"],
      "package.json": ["../package.json"]
    }
  }
}
