{
  "name": "temzone",
  "description": "The Temtem Interactive Map API",
  "version": "0.1.0-a.7",
  "author": "Temtem Interactive Map",
  "license": "MIT",
  "type": "module",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "scraper": "node scraper/index.js",
    "database:migrate": "wrangler d1 migrations apply temzone --local --env dev",
    "build": "node build.js",
    "dev": "wrangler dev --local --persist --env dev",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint . --ext .js,.json,.yml",
    "lint:fix": "eslint --fix . --ext .js,.json,.yml",
    "test": "npm run build && firebase emulators:exec --project temzone --import=emulator-data vitest",
    "deploy": "wrangler publish"
  },
  "dependencies": {
    "@lyrasearch/lyra": "0.4.11",
    "hono": "3.0.3",
    "jose": "4.13.1",
    "kysely": "0.23.5",
    "kysely-d1": "0.3.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "better-sqlite3": "8.2.0",
    "cheerio": "1.0.0-rc.12",
    "command-line-args": "5.2.1",
    "esbuild": "0.17.11",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-yml": "1.5.0",
    "firebase-tools": "11.24.1",
    "picocolors": "1.0.0",
    "prettier": "2.8.4",
    "prettier-eslint": "15.0.1",
    "prettier-plugin-sql": "0.13.0",
    "sharp": "0.31.3",
    "uuid": "9.0.0",
    "vite-jsconfig-paths": "2.0.1",
    "vitest": "0.29.2",
    "wrangler": "2.12.2"
  }
}
