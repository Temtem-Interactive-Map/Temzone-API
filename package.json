{
  "name": "temzone",
  "description": "The Temtem Interactive Map API",
  "version": "0.1.0-a.5",
  "author": "Temtem Interactive Map",
  "license": "MIT",
  "type": "module",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "scraper": "node scraper/index.js",
    "database:update": "prisma db push",
    "database:insert": "node scraper/database/index.js",
    "firebase": "firebase emulators:start --project temzone --import=emulator-data --export-on-exit=emulator-data",
    "build": "node build.js",
    "dev": "miniflare --live-reload --watch --debug --modules dist/index.mjs",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint . --ext .js,.json,.yml",
    "lint:fix": "eslint --fix . --ext .js,.json,.yml",
    "test": "npm run build && cross-env NODE_OPTIONS=--experimental-vm-modules firebase emulators:exec --project temzone --import=emulator-data vitest",
    "deploy": "wrangler publish"
  },
  "dependencies": {
    "hono": "3.0.2",
    "jose": "4.13.0"
  },
  "devDependencies": {
    "@prisma/client": "4.11.0",
    "cheerio": "1.0.0-rc.12",
    "command-line-args": "5.2.1",
    "cross-env": "7.0.3",
    "esbuild": "0.17.10",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-yml": "1.5.0",
    "firebase-tools": "11.24.0",
    "miniflare": "2.12.1",
    "picocolors": "1.0.0",
    "prettier": "2.8.4",
    "prettier-eslint": "15.0.1",
    "prisma": "4.11.0",
    "sharp": "0.31.3",
    "uuid": "9.0.0",
    "vitest": "0.29.2",
    "vitest-environment-miniflare": "2.12.1",
    "wrangler": "2.11.1"
  }
}
