{
  "name": "temzone",
  "description": "The Temtem Interactive Map API",
  "version": "0.1.0-a.2",
  "author": "Temtem Interactive Map",
  "license": "MIT",
  "type": "module",
  "main": "src/api/index.js",
  "private": true,
  "scripts": {
    "scraper": "node scraper/index.js",
    "database:update": "prisma db push",
